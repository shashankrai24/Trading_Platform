<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>

<body>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">index</th>
            <th scope="col">Crypto</th>
            <th scope="col">open</th>
            <th scope="col">high</th>
            <th scope="col">low</th>
            <th scope="col"> BUY-O</th>
            <th scope="col">SELL-O</th>
            <th scope="col">Current-position-O</th>
            <th scope="col">order-history-O</th>
          </tr>
        </thead>
        <tbody>
           <% 
            counter=0
           crypto['data'].forEach(function(cryptos)
           {
               counter++;
            %><tr>
            <th scope="row"><%=counter%></th>
            <td><%=cryptos.instId%></td>
            <td><%=cryptos.open24h%></td>
            <td><%=cryptos.high24h%></td>
            <td><%=cryptos.low24h%></td>
            <td><a href="/optionorderplacement?instId=<%= cryptos.instId %>" class="btn btn-success">
                BUY
            </a></td>
            <td><a href="/selloptionorderplacement?instId=<%= cryptos.instId %>" class="btn btn-danger">
              SELL
          </a></td>
            <td> <a href="/optiontradingposition?instId=<%= cryptos.instId %>" class="btn btn-warning">
                position
            </a></td>
          <td> <a href="/optionorderhistory?instId=<%= cryptos.instId %>" class="btn btn-success">
            order-history
        </a></td>
          </tr> 
            <%})
             
            %>
        </tbody>
      </table>
</body>
</html>