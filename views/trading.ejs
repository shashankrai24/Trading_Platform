<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>

<body>
  <div style="display: flex; justify-content: center; align-items: center; width: 100%;text-align: center; background-color: #f8f9fa; margin-bottom: 20px;">
    <a href="/futures" style="text-decoration: none; color: white; background-color: #007bff; padding: 10px 20px; border-radius: 5px; display: inline-block;">
        <h4 style="margin: 0;">F/O Trading</h4>
    </a>
</div>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Crypto</th>
            <th scope="col">open</th>
            <th scope="col">high</th>
            <th scope="col">low</th>
            <th scope="col">BUY-SPOT</th>
            <th scope="col">SELL-SPOT</th>
            <th scope="col">Current-position</th>
            <th scope="col">order-book</th>
            <th scope="col">fulfilled-orders</th>
            <th scope="col">live-updates</th>
          </tr>
        </thead>
        <tbody>
           <% 
            counter=0
           crypto['data'].forEach(function(cryptos)
           {
               counter++;
            %><tr>
            <th scope="row"><%=counter%></th>
            <td><%=cryptos.instId%></td>
            <td><%=cryptos.open24h%></td>
            <td><%=cryptos.high24h%></td>
            <td><%=cryptos.low24h%></td>
            <td><a href="/orderplacement?instId=<%= cryptos.instId %>" class="btn btn-success">
                BUY
            </a></td>
            <td><a href="/sellplacement?instId=<%= cryptos.instId %>" class="btn btn-danger">
              SELL
          </a></td>
            <td> <a href="/position?instId=<%= cryptos.instId %>" class="btn btn-warning">
                position
            </a></td>
            <td> <a href="/orderbook?instId=<%= cryptos.instId %>" class="btn btn-secondary">
              order-book
          </a></td>
          <td> <a href="/pendingorders?instId=<%= cryptos.instId %>" class="btn btn-success">
            fulfilled-orders
        </a></td>
        <td> <a href="/liveupdates?instId=<%= cryptos.instId %>" class="btn btn-primary">
          live-updates
      </a></td>
          </tr> 
            <%})
             
            %>
        </tbody>
      </table>
</body>
</html>